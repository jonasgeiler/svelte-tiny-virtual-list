{
	"name": "@tutorlatin/svelte-tiny-virtual-list",
	"version": "3.0.4",
	"description": "A maintained fork of svelte-tiny-virtual-list",
	"homepage": "https://github.com/TutorLatin/svelte-tiny-virtual-list#readme",
	"bugs": "https://github.com/TutorLatin/svelte-tiny-virtual-list/issues",
	"license": "MIT",
	"author": "Jonas Geiler <npm@jonasgeiler.com> (https://jonasgeiler.com)",
	"repository": "github:TutorLatin/svelte-tiny-virtual-list",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "npm run test:integration && npm run test:unit",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.53.0",
		"@sveltejs/adapter-cloudflare": "^7.0.4",
		"@sveltejs/kit": "^2.21.5",
		"@sveltejs/package": "^2.3.11",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@types/eslint": "^9.6.1",
		"beercss": "^3.11.19",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.2",
		"marked": "^15.0.12",
		"marked-base-url": "1.1.6",
		"marked-gfm-heading-id": "4.1.1",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.12",
		"svelte": "^5.34.5",
		"svelte-check": "^4.2.1",
		"svelte-infinite-loading": "1.4.0",
		"tslib": "2.8.1",
		"typescript": "^5.8.3",
		"vite": "^6.3.5",
		"vitest": "^3.2.4"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"type": "module",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"engines": {
		"node": ">=22"
	},
	"keywords": [
		"svelte",
		"virtual",
		"list",
		"scroll",
		"infinite",
		"loading",
		"component",
		"plugin",
		"svelte-components"
	]
}
